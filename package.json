{
	"name": "@undb/n8n-nodes-undb",
	"version": "0.2.0",
	"description": "n8n community node undb",
	"keywords": [
		"n8n-community-node-package"
	],
	"license": "MIT",
	"homepage": "",
	"author": {
		"name": "nichenqin",
		"email": "nichenqin@hotmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/undb-xyz/n8n-nodes-undb.git"
	},
	"main": "index.js",
	"scripts": {
		"commit": "cz",
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json",
		"release": "release-it"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/UndbApi.credentials.js",
			"dist/credentials/UndbAuthApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Undb/Undb.node.js"
		]
	},
	"devDependencies": {
		"@types/express": "^4.17.6",
		"@types/request-promise-native": "~1.0.15",
		"@typescript-eslint/parser": "~7.17.0",
		"commitizen": "^4.3.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint-plugin-n8n-nodes-base": "^1.11.0",
		"gulp": "^4.0.2",
		"n8n-core": "0.176.0",
		"n8n-workflow": "0.157.0",
		"prettier": "^3.0.0",
		"release-it": "^16.2.1",
		"typescript": "~5.2.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
